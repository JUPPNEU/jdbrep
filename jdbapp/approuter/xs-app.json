{	
	"welcomeFile": "/app/resources/index.html",
	"authenticationMethod": "route",
	"sessionTimeout": 30,
	"logout": {
	"logoutEndpoint": "/do/logout",
	"logoutPage": "/"
 },
	"routes": [
 	{
	"source": "^/app/(.*)$",
	"target": "$1",
	"localDir": "resources",
	"authenticationType": "xsuaa",
	"csrfProtection" : false
	}, 
	{
		"source": "^/odata/(.*)$",
		"target": "$1",
		"localDir": "resources",
		"authenticationType": "xsuaa",
		"csrfProtection" : false
		}, 
	{
	"source": "^/catalog/(.*)$",
	"destination": "srv-binding",
	"authenticationType": "xsuaa",
	"csrfProtection" : false
	}
 ]
}